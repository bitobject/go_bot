# Telegram Bot Token (получи у @BotFather)
TELEGRAM_TOKEN=YOUR_ACTUAL_TOKEN_HERE

# PostgreSQL Database Configuration
POSTGRES_DB=goooo
POSTGRES_USER=goooo_user
POSTGRES_PASSWORD=goooo_password
POSTGRES_HOST=postgres
POSTGRES_PORT=5432

# Application Database URL
DATABASE_URL=postgres://goooo_user:goooo_password@postgres:5432/goooo?sslmode=disable

# Nginx Configuration (опционально)
NGINX_SERVER_NAME=body-architect.ru
NGINX_SSL_CERT_PATH=/etc/letsencrypt/archive/body-architect.ru/fullchain1.pem
NGINX_SSL_KEY_PATH=/etc/letsencrypt/archive/body-architect.ru/privkey1.pem
